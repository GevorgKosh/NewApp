<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/purple_0" />
            <stroke android:color="@color/black" android:width="@dimen/dimen_1"/>
            <corners
                android:radius="@dimen/dimen_30" />
            <padding
                android:left="@dimen/dimen_10"
                android:top="@dimen/dimen_7"
                android:right="@dimen/dimen_10"
                android:bottom="@dimen/dimen_7" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white" />
            <stroke android:color="@color/black" android:width="@dimen/dimen_1"/>
            <corners
                android:radius="@dimen/dimen_30" />
            <padding
                android:left="@dimen/dimen_10"
                android:top="@dimen/dimen_7"
                android:right="@dimen/dimen_10"
                android:bottom="@dimen/dimen_7" />
        </shape>
    </item>
</selector>